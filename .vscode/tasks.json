{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Gradle: TeamCode assembleDebug",
      "type": "shell",
      "command": "${workspaceFolder}\\gradlew.bat",
      "args": [":TeamCode:assembleDebug", "--no-daemon"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [],
      "group": "build"
    },
    {
      "label": "Gradle: clean",
      "type": "shell",
      "command": "${workspaceFolder}\\gradlew.bat",
      "args": ["clean", "--no-daemon"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [],
      "group": "build"
    }
  ]
}
